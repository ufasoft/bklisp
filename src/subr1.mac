TRUNCA::
	TST	(R0)+
FLOOR::	CMP	(R0)+, (R0)+
EXP::	TST	(R0)+
ATAN::	TST	(R0)+
TAN::	TST	(R0)+
COS::   TST	(R0)+
SIN::	TST	(R0)+
