;; (COSH number) => result
COSH::	MOV	$APLMI(R0), 1$
	PU.FL
	.BASN
	.WORD	DDUP, $EXP, DSWAP, DNEG, $EXP
1$:	.WORD	$DADD, 2$			;Self-modifying code
2$:     TST	(SP)				; /2 FLT4 on Stack, POP into R1 and Return
	BEQ	9$
	SUB	#200, (SP)		;!!!CHECK check for min exponent
9$:	POFL.R


;; (TANH number) => result
TANH::	MOV	R1, R2
	.SUBR	COSH
	.SWP12
	.SUBR	SINH
	.BASN
	.WORD	DPUR12, $DDIV, RBAS.5



