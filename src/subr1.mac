	BR	MATH
ACOS::	ADD	#ACOS1 - ASIN1, R0
ASIN::	ADD	#ASIN1, R0
MATH:	PU.FL
	MOV	#DREFLT, R4
        JMP	(R0)


;; (SINH number) => result
