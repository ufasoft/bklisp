CPSTR1:	MOV	R1, R5
	BISB	-3(R1), R0	; R0: length
	MOV	R0, R1
	.SUBR	MKSTR1
	MOV	R1, R4
	ASR	R0		; # of words
	INC	R0
	CMPB	-(R5), -(R4)
	CMP	-(R5), -(R4) 	; R4: dst, R5: src
	BR	CPSEQ1


;; (GRAPHIC-CHAR-P char) => bool
