;; (CHAR-UPCASE character) => corresponding-character
CHRUP::	ADD	#200, R0	; BPL opcode
	MOVB	R0, CHCAI+1
	.PRED	ALCHP		; (ALPHA-CHAR-P character)
	BCC	CHCAI1		; non-ALPHA-CHAR-P
	TSTB	R1
CHCAI:	BPL	2$		; SELF-MODIFYING CODE!
	BIS	#40, R1		; RUS
	BR	CHCAI1
