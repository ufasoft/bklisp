;; (FIND-SYMBOL string) => symbol, state (in CF)
FINSY::	MOV	#VS.NIL, R2
	.STREQ		; (STRING= string "NIL")
	BCS	R.R0A
;	BCC	1$
;	CLR	R1		; RETURN NIL
;	BR	3$
1$:     MOV	R1, R2
	CALL	STR2R		; RASIC-50 failed, fallback to ASCII
	MOV	#OBLIST - 3, R1
	BCS	5$		; ASCII
6$:
	MOV	3(R1), R1
	BEQ	7$	        ; Not found
	MOV	R1, R5		; Compare RADIX-50 strings
	ADD	#5, R5
	MOV	#$BUFOUT, R4
.IF DF C$RAD40
8$:	CMP	(R5)+, (R4)	; ASCII names starts with ZERO word and will be filtered-out
	BNE	6$		; strings differ
	TST	(R4)+
	BMI	8$		; more words
.IFF
8$:	CMP	(R5)+, (R4)+	; ASCII names starts with ZERO word and will be filtered-out
	BNE	6$		; strings differ
	CMP	R4, R3
	BNE	8$
.ENDC
	BR	3$
5$:	ADD	#12, R1

2$:	MOV	-7(R1), R1
	BNE	4$
7$:	CLC       ; Not found
	RETURN
4$:	ADD	#12, R1		; R2: SYMBOL-NAME
	.STREQ
	BCC	2$
	SUB	#12, R1
3$:	SEC
9$:	RETURN

