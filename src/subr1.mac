;; (GENSYM &optional x) => new-symbol
GENSYM::
	.DF1.NIL
	.PUSH0
	.INTP
	BCS	2$		; explicit suffix
	MOV	#V.GCNT, R1     ; *GENSYM-COUNTER*
	.GPLAC
	MOV	(R4), R1
	MOV	(R0), R3	; R3: x
	MOV	R1, (R0)	; replace x
	.SUBR	INCR
	MOV	R1, (R4)	; (SETQ *GENSYM-COUNTER* (1+ *GENSYM-COUNTER*))
	MOV	R3, R1		; R1: x
	.STRP
	BCS	3$
2$:	MOV	#V.C.G, R1	; default 'G' prefix
3$:	.PUSH0
	MOV	#V.GSYM, R1
	.PUSH0
