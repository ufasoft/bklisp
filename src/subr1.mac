;; (COPY-SEQ sequence) => copied-sequence
CPSEQ::	.LISTP
	BCS	CPLIST
	.SVECP
	BCC	CPSTR
.IF NDF K1801
	DEC	R1
.ENDC
	MOV	R1, R5		; R5 src buf
	MOV	(R1)+, R1
	MOV	R1, R0
	.MKAR
	SUB	#FIXOFF-1, R0
	BEQ	CPSEQ2
	MOV	R1, R4  	; R4: dest buf
.IF NDF K1801
	DEC	R4
.ENDC
CPSEQ1:	MOV	(R5)+, (R4)+
	SOB	R0, CPSEQ1
