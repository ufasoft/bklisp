	MOV	R2, (R1)
	BR	R.M31B		; MOV R3, R1



;; (RASSOC item alist) => entry
RASSOC::
	TST	(R0)+		; R0 = 2 (Offset to CDR)
;; (ASSOC item alist) => entry
;; ASSUME R0 == 0
ASSOC::	MOV	R2, R5		; R5: alist
	MOV	R1, R2		; R2: item
1$:	.POP5
        BEQ.F
        T.CONS
        MOV	R1, R3		; R3: entry
        ADD	R0, R1
        MOV	(R1), R1	; R1: (CAR/CDR entry)
        .EQL
        BCC	1$		; Test failed
R.M31B:	BR	R.M31


;; (MEMBER item list) => tail
MEMBER::
	MOV	R2, R5		; R5: list
	MOV	R1, R2		; R2: item
1$:	MOV	R5, R3
	.POP5
	BEQ	R.M31
	.EQL
	BCC	1$
R.M31:	MOV	R3, R1
	RETURN

;; (REMOVE item seq) => result-seq
REMOVE::
	.SWP12			; R2: item
	.LISTP
	BCS	3$		; seq is List
	MOV	R2, R3		; R3: item
	MOV	#V.LIST, R2
	.SUBR	COERCE		; R1: (COERCE seq 'list)
	MOV	R3, R2		; R2: item
	PUSH	#VECTO1		; (APPLY #'VECTOR (REMOVE item (COERCE seq 'list)))
3$:	MOV	R1, R5
1$:	.POP5
	BEQ	2$
	.EQL
	BCS	1$
	.PUSH0
	BR	1$
2$:	JMP	R.NREV		; .NREV0; RETURN


;; (SETF (SYMBOL-FUNCTION symbol) new-value)
X.SYFU::
	T.SYM
	MOV	R2, 1(R1)
R.M21:	MOV	R2, R1
R.A:	RETURN



;; (SETF (THIRD list) value) => value
X.THIRD::
	INC	R0
X.SECOND::
	ADD	#V.1, R0
	MOV	R2, R3		; R3: value
	.SH210
	BR	X.NTH
