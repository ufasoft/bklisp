;; (READ-CHAR &optional input-stream eof-error-p eof-value) => char
R.CHAR::
	TO.STM
	MOV	R1, R4
	INC	R4
.IF NE C$FILE
	.TYPEP	U.FILE
	BCC	1$
	TST	(R4)+		; R4: &position
	MOV	(R4)+, R1	; R1: position as Object
	TST	(R4)+		; R4: &channel
	TO.U16
	BIT	#777, R1
	BNE	4$
	MOV	#S$REA * 256., R3
	CALL	REAWRI
	BCS	R.EOF
4$:	MOV	R1, R2
	INC	R1   		; ++filepos
	F.U16
	MOV	R1, -4(R4)	; save new filepos
	BIC	#^C777, R2	; R2: offset in buffer
	ADD	R2, R4		; R4: pointer to next char - 2
	MOV	#V.CHB, R1
	BISB	2(R4), R1
;	BEQ	R.EOF
	RETURN
.ENDC ; C$FILE

1$:	MOV	(R4)+, R1
	.STRP
	BCC	GETCH		; STDIN
	MOV	(R4)+, R3	; R3: cur pos
	CMP	R3, (R4)	; (R4): upper bound
	BHIS	R.EOF		; EOF
	INC	R3
	MOV	R3, -(R4)	; increment cur pos
	ADD	#-FIXOFF-3, R3
	BR	CHAR8		; ADD R1, R3;  MOV #V.CHB, R1;  BISB (R3)+, R1
R.EOF:	TST	R2		; eof-error-p
	BNE.E
R.EOF1:	MOV	(R5), R1	; R1: eof-value
RCHAR9:	RETURN



;; (FILL sequence item) => sequence
FILL::	MOV	R2, R3		; R3: item
	MOV	R1, R0
	.LENGTH
	.SH210			; R1: sequence, R2: (LENGTH sequence)
1$:	DEC	R2
	CMP	R2, #V.0
	BLO	RCHAR9		; RETURN
	.PRED	X.ELT
	BR	1$

;; (REDUCE function list) => result
REDUCE::
	TO.FUN
	MOV	R1, R4		; SAVE function in R4
	MOV	R2, R5
	.POP5
	BNE	1$		; NON-EMPTY LIST OF ARGS
	MOV	R4, R1		; function
	JMP	FUNCALL		; (FUNCALL function)
2$:	CLR	R2
	.CONS
	.SH210
	.CONS          		; R1: LIST OF 2 ARGS
	MOV	R5, R3
	MOV	R1, R5
	MOV	R4, R1
	.FUNCALL		; (FUNCALL a b)
	MOV	R3, R5
1$:	MOV	R1, R0		; ACCUMULATE RESULT IN R0
	.POP5
	BNE	2$		; MANY ARG
R.M01:	MOV	R0, R1
	RETURN


;; (SORT sequence predicate) => sorted-sequence
SORT::	MOV	R1, R3
	.LENGTH
	MOV	R1, R5		; R5: length
	MOV	R3, R1
	.SVECP
	BCC	4$      	; Sort List
	SUB	#FIXOFF + 1, R5	; Sort Vector
	BLOS	9$
6$:	TST	(R3)+
	MOV	R3, R4
	PUSH	R5
7$:	TST	(R4)+
	.SORT34
	SOB	R5, 7$
	POP	R5
	SOB	R5, 6$
9$:	RETURN
3$:	MOV	2(R3), R3
	BEQ	9$
4$:	MOV	R3, R4
2$:	MOV	2(R4), R4
	BEQ	3$
	.SORT34
	BR	2$




