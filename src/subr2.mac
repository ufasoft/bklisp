;; (RANDOM limit &optional random-state) => random-number
RANDOM::
	PU.FL
	PU.FL 			; Positive to get random
	MOV	R2, R1
	CMP	R1, R4		; compare with V.U
	BNE	1$
	.SYVAL	V.RNDS		; *RANDOM-STATE*
1$:	MOV	(R1)+, R2
	TST	-(R2)
	MOV	R2, A.RDNS
	MOV	#$III, R3
	CALL	COPY4
	MOV	(R1)+, R2
	TST	-(R2)
	MOV	R2, B.RDNS
	.BASN
	.WORD	DCP23, $RAN, $DMUL, B.III, RBAS.5 		; SCALE



