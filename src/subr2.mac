; Sort (R3), (R4) by Predicate IN R2
SORT34: MOV	R2, R0
	MOV	(R3), R1
	MOV	(R4), R2
	.PRED	CALL3		; (FUNCALL R0 R1 R2)
	BCS	1$		; R1 < R2
	MOV	(R3), R1      	; swap @(R3), (R4)
	MOV	(R4), (R3)
	MOV	R1, (R4)
1$:	RETURN



