;; (SCALE-FLOAT float integer) => scaled-float
SCALEF::
	PU.FL
	MOV	R2, R1
	TO.I16
	MOV	(SP), R2
	ASL	R2
	CLRB	R2
	SWAB	R2		; R3: exponent
	ADD	R2, R1		; R1: new exponent
	SWAB	R1		; CF: 0
	BNE.E			; resulting exponent is out of range
	ROR	R1
	BIC	#77600, (SP)	; Replace exponent in Stack
	BIS	R1, (SP)
	POFL.R


