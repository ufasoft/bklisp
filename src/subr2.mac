;; (SVREF simple-vector index) => element
SVREF::	T.VEC
	.SWP12
.IIF EQ C$VM1, DEC	R2
	MOV	(R2)+, R0	; R0: vector length
	SUB	#FIXOFF, R0
SVREF1:	TO.U16
	CMP	R1, R0
	BHIS	E.RANG
	ASL	R1
	ADD	R1, R2
	MOV	(R2), R1
	RETURN

;; (DIGIT-CHAR-P char &optional (radix 10)) => weight
DIGCHP::
	.DF2	V.10
	.CHRUP		; R1: (CHAR-UPCASE char)
	ADD	#FIXOFF - V.CHB - '0, R1
	.TI16B
	CMP	R1, R0
	BLO.F				; char < '0
	CMP	R1, #9.
	BLOS	1$
	CMP	R1, #'A - '0
	BLO.F
	ADD	#10. + '0 - 'A, R1
1$:	CMP	R1, R2		; R1: weight as int16
	BHIS.F			; weight >= radix
AFIX.R:	ADD	#FIXOFF, R1
	RETURN


;; (CHAR string index) => character
CHAR::	SUB	#3, R1
	BISB	(R1)+, R0
	.SWP12
	TO.U16
	CMP	R1, R0
	BHIS	E.RANG
	MOV	R2, R3
CHAR8:	ADD	R1, R3
	JMP	RS.CH8		; MOV #V.CHB, R1;  BISB (R3)+, R1




